{
  "name": "browser-agent",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "electron/main.cjs",
  "build": {
    "appId": "com.zsq.browser-agent",
    "productName": "browser-agent",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "mac": {
      "icon": "src-tauri/icons/icon.icns"
    },
    "win": {
      "icon": "src-tauri/icons/icon.ico"
    },
    "linux": {
      "icon": "src-tauri/icons/icon.png",
      "target": [
        "AppImage"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently -k \"vite\" \"wait-on http://localhost:1420 && electron ./electron/main.cjs\"",
    "dev:electron:remote": "electron ./electron/main.cjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pack": "pnpm build && electron-builder --dir",
    "dist": "pnpm build && electron-builder"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.1.2",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "wait-on": "^8.0.3"
  }
}
